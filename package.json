{
    "private": true,
    "scripts": {
        "production": "webpack --env NODE_ENV=production --env production",
        "production:watch": "webpack --env NODE_ENV=production --env production --watch",
        "dev": "webpack --env NODE_ENV=development",
        "watch": "webpack --env NODE_ENV=development --watch",
        "translate": "wp-pot --src '**/**/**/*.php' --dest-file 'languages/wordpress-webpack.pot' --package '{{the-project-name}}' --domain '{{the-project-text-domain}}' --last-translator '{{author_name}} <{{author_email}}>' --team '{{author_name}} <{{author_email}}>' --bug-report '{{author_url}}'"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.18.7",
        "eslint-webpack-plugin": "^3.1.1",
        "node-sass-magic-importer": "^5.3.2",
        "prettier": "^2.6.2",
        "stylelint-webpack-plugin": "^3.3.0",
        "tailwindcss": "^3.0.24",
        "typescript": "^4.7.3",
        "vue": "^3.2.37",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2"
    }
}
