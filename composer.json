{
    "name": "buckaroo/woocommerce",
    "description": "Buckaroo WooCommerce Plugin",
    "version": "4.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "type": "wordpress-plugin",
    "license": "CC-BY-NC-ND-3.0",
    "autoload": {
        "psr-4": {
            "Buckaroo\\WooCommerce\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "micropackage/requirements": "^1.0",
        "inpsyde/modularity": "^1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "wp-coding-standards/wpcs": "*",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "inpsyde/php-coding-standards": "^1.0",
        "symfony/var-dumper": "^5.4"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "boilerplate",
        "framework"
    ],
    "authors": [
        {
            "name": "Buckaroo",
            "email": "support@buckaroo.nl",
            "homepage": "https://www.buckaroo.nl"
        }
    ],
    "support": {
        "email": "support@buckaroo.nl",
        "issues": "https://support.buckaroo.nl/contact"
    },
    "homepage": "https://www.buckaroo.nl",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
